<?xml version="1.0"?>
<robot name=" test_urdf ">
<link name="base_link">
    <visual>
      <geometry>
            <cylinder length="0.6" radius="0.2"/>
      </geometry>
    </visual>
  </link>

 <link name="link_2">
    <visual>
      <geometry>
             <cylinder length="0.6" radius="0.2"/>
      </geometry>
    </visual>
  </link>
   <link name="camera_link">
    <visual>
      <geometry>
             <box size="0.1 0.1 0.05"/>
      </geometry>
    </visual>
 </link>
        <joint name="base_to_link_2" type="revolute">
        <parent link="base_link"/>
        <child link="link_2"/>
	 <origin xyz="0.6 0.0 0.0" rpy="0.0 0.0 0.0"/>
 </joint>
        <joint name="link_2_to_camera" type="fixed">
        <parent link="link_2"/>
        <child link="camera_link "/>
        <origin xyz="0.6 0.0 0.0" rpy="0.0 0.0 0.0"/>
 </joint>
</robot>
